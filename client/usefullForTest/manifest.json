{
  "manifest_version": 2,
  "name": "Uptex",
  "version": "1.1",
  

  "description": "Pin on the top your favorite channels on twitch",

  "permissions": ["*://*.twitch.tv/*"],
  "web_accessible_resources":["index.js"],
  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["*://*.twitch.tv/*"],
      "js": ["./index.js"]
    }
  ]
}